1)
#include <stdio.h>

int main() {
   
    for (int N = 0; N <= 20; N++) {
       
        if (N % 2 != 0) {
            printf("%d\n", N); 
        }
        
    }

    return 0;  
}




2)
#include <stdio.h>

int main() {
    int soma = 0; 

    for (int N = 1; N <= 100; N++) {
        soma += N;  
    }
    
    printf("a soma dos 100 primeiros números é: %d\n", soma);

    return 0; 
}

